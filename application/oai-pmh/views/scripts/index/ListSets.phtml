<ListSets>
<?php foreach ($this->tenants as $tenant):
	$rdf = $tenant->toRdf();
?>
  <set>
    <setSpec><?php echo $tenant->code?></setSpec>
    <setName><?php echo $this->escape($tenant->name)?></setName>
    <setDescription>
    	<?php echo $rdf->saveXml($rdf->documentElement)?>
    </setDescription>
  </set>
<?php endforeach?>
<?php foreach ($this->collections as $collection):
	$rdf = $collection->toRdf();
?>
  <set>
    <setSpec><?php echo $collection->id?></setSpec>
    <setName><?php echo $this->escape($collection->dc_title)?></setName>
    <setDescription>
    	<?php echo $rdf->saveXml($rdf->documentElement)?>
    </setDescription>
  </set>
<?php endforeach?>
</ListSets>